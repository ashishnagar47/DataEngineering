{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Mission100ashish30121998"
		},
		"ADLS_assignment1_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_assignment1_LS'"
		},
		"SQL_DB_assignment1_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_DB_assignment1_LS'"
		},
		"ADLS_assignment1_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ashish30121998adls.dfs.core.windows.net/"
		},
		"SQL_DB_assignment1_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mission100ashish.database.windows.net"
		},
		"SQL_DB_assignment1_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Mission100ashish30121998DB"
		},
		"SQL_DB_assignment1_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ashish"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Mission100ashish30121998_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_assignment1_LS_accountKey": {
						"value": "[parameters('ADLS_assignment1_LS_accountKey')]"
					},
					"SQL_DB_assignment1_LS_password": {
						"value": "[parameters('SQL_DB_assignment1_LS_password')]"
					},
					"ADLS_assignment1_LS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_assignment1_LS_properties_typeProperties_url')]"
					},
					"SQL_DB_assignment1_LS_properties_typeProperties_server": {
						"value": "[parameters('SQL_DB_assignment1_LS_properties_typeProperties_server')]"
					},
					"SQL_DB_assignment1_LS_properties_typeProperties_database": {
						"value": "[parameters('SQL_DB_assignment1_LS_properties_typeProperties_database')]"
					},
					"SQL_DB_assignment1_LS_properties_typeProperties_userName": {
						"value": "[parameters('SQL_DB_assignment1_LS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Mission100ashish30121998_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Mission100ashish30121998_ArmTemplate_0')]"
			]
		}
	]
}